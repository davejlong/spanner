{
  "templates": [
    {
      "source": "{{#.}}\n  {{title}}\n  {{url}}\n  ```{{body}}```\n{{/.}}\n",
      "path": "templates/slack/pr.mustache",
      "name": "pr",
      "adapter": "slack"
    },
    {
      "source": "{{#.}}\n  {{title}}\n  {{url}}\n  ```{{body}}```\n{{/.}}\n",
      "path": "templates/slack/prs.mustache",
      "name": "prs",
      "adapter": "slack"
    },
    {
      "source": "{{#.}}\n  {{name}} {{#parent}}(forked from {{name}}){{/parent}}\n  {{html_url}}\n{{/.}}\n",
      "path": "templates/slack/repo.mustache",
      "name": "repo",
      "adapter": "slack"
    }
  ],
  "permissions": [],
  "rules": [],
  "commands": [
    {
      "version": "0.0.1",
      "options": [
        {
          "type": "string",
          "required": false,
          "name": "format"
        }
      ],
      "name": "date",
      "env_vars": [],
      "executable": "/bin/date",
      "enforcing": false,
      "calling_convention": "bound",
      "documentation": "date [--format=<date_format>] [date] - Returns current date or formats a user-specified date",
      "calling_convention": "bound"
    }
  ],
  "bundle": {
    "name": "date",
    "type": "foreign"
  }
}
